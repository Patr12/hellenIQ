    "def fasscri_bd(n , memo={}):\n",
    "   if n in memo:\n",
    "      return memo[n]\n",
    "   if n <= 1:\n",
    "      return n\n",
    "   memo[n] = fasscri_bd(n-1, memo) + (n-2, memo)\n",
    "   return memo[n]\n",
    "   print(fasscri_bd(50))"
   ]
  }
